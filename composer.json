{
    "name": "web-chefs/db-lojack",
    "type": "library",
    "description": "Laravel Database Query Logger and debug helper.",
    "keywords": [
        "db-lojack",
        "laravel",
        "logging",
        "database",
        "debugging"
    ],
    "homepage": "https://github.com/web-chefs/DB-LoJack",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Fossey"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "laravel/framework": "5.*",
        "symfony/stopwatch": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "^2.3",
        "doctrine/dbal": "^2.5",
        "web-chefs/laravel-app-spawn": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WebChefs\\DBLoJack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebChefs\\DBLoJack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebChefs\\DBLoJack\\DBLoJackServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}